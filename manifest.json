{
    "manifest_version": 2,
    "name": "SharePoint Population Tool",
    "version": "1.1.0",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_title": "Populate List",
        "default_icon": "popup/page_32.png"
    },
    "externally_connectable": {
        "matches": [
            "*://*.sharepoint.com/*"
        ]
    },
    "web_accessible_resources": [
        "assets/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    }
}